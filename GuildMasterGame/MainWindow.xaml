<Window x:Class="GuildMasterGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GuildMasterGame"
        mc:Ignorable="d"
        Title="Start screen" Height="450" Width="800">
    <Grid>
        <Image Source="resources\GuildMasterTitleScreen.png" Stretch="Fill"/>
        <Image Source="resources\TherapyGames.png" Margin="582,276,-40.4,10" OpacityMask="Black"/>
        <Button x:Name="StartButton"  Style="{StaticResource TransparentButton}" Content="Start" HorizontalAlignment="Center" Margin="210,253,183.6,0" VerticalAlignment="Top" Width="400" RenderTransformOrigin="0.503,0.525" Height="70" Click="StartButton_Click" Foreground="White" />
        <Label x:Name="GuildMasterLabel" Content="Guild Master" HorizontalAlignment="Left" Margin="261,28,0,0" VerticalAlignment="Top" Height="63" Width="279" FontFamily="Gill Sans Nova" FontSize="48" Background="Black" Foreground="#FFFF5706"/>
    </Grid>
    
    
</Window>
